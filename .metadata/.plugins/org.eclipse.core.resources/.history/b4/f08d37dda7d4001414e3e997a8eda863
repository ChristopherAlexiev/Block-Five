package com.bayviewglen.test;

import java.util.HashMap;

import com.bayviewglen.datastructures.Cell;
import com.bayviewglen.datastructures.Cell_3D;
import com.bayviewglen.utils.Utils;

public class Test3DConversion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		int cols = 5;
		int layers = 4;
		HashMap<Cell_3D, Integer> cellVertexMap = Utils.create3DGridHashMap(rows,cols,layers);
    	HashMap<Integer, Cell_3D> vertexCellMap = Utils.create3DVertexHashMap(rows,cols,layers);
    	
    		
    			Cell_3D v = vertexCellMap.get(27);
    			Integer j = cellVertexMap.get(new Cell(3,3,5));
    			
    			System.out.println(v + "\n" + j);
    			
	}

}
